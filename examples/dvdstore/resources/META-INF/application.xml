<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee" 
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
             version="5">
    
    <display-name>DVD Store</display-name>
    
    <module>
        <ejb>jboss-seam-dvd.jar</ejb>
    </module>
    
    <module>
        <web>
            <web-uri>jboss-seam-dvd.war</web-uri>
            <context-root>/seam-dvd</context-root>
        </web>
    </module>
    
    <module>
        <ejb>jboss-seam.jar</ejb>
    </module>

    <module>
        <java>jbpm-jpdl.jar</java>
    </module>
    
    <module>
        <java>jboss-el.jar</java>
    </module>
    
    <module>
        <java>lucene-core-2.1.0.jar</java>
    </module>
    
    <module>
        <java>hibernate-search.jar</java>
    </module>
    
    <module>
        <java>hibernate-commons-annotations.jar</java>
    </module>
    
</application>
